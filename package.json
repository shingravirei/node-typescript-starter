{
    "name": "ts",
    "version": "0.1.0",
    "description": "A node/typescript project template",
    "main": "index.ts",
    "scripts": {
        "dev": "NODE_ENV=development nodemon --watch \"src/**\" --ext \"ts,json\"  --exec \"ts-node src/main.ts\"",
        "check": "tsc",
        "build": "tsc -b",
        "start": "node ./dist/index.js",
        "test": "jest --watch",
        "clean": "rm -rf ./dist/*"
    },
    "keywords": [
        "typescript",
        "node",
        "project",
        "starter"
    ],
    "author": "shingravirei",
    "license": "MIT",
    "devDependencies": {
        "@swc/core": "^1.2.160",
        "@swc/helpers": "^0.3.8",
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.23",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "eslint": "^8.12.0",
        "jest": "^27.5.1",
        "nodemon": "^2.0.15",
        "ts-jest": "^27.1.4",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.3"
    }
}
